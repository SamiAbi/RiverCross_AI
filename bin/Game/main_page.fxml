<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="668.0" prefWidth="900.0"
	style="-fx-background-color: #734d26;"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="Game.GameController">
	<children>

		<Pane fx:id="riverpnl" layoutY="99.0" prefHeight="480.0"
			prefWidth="900.0" style="-fx-background-color: #66b3ff;" />
		<ImageView fx:id="woodimg" fitHeight="40.0" fitWidth="100.0"
			layoutX="798.0" pickOnBounds="true" smooth="false">
			<image>
				<Image url="@w1.png" />
			</image>
		</ImageView>
		<Label fx:id="woodlbl" layoutX="735.0" layoutY="7.0"
			text="Wood - " textFill="WHITE">
			<font>
				<Font size="19.0" />
			</font>
		</Label>
		<ImageView fx:id="rick" fitHeight="46.0" fitWidth="50.0"
			layoutX="426.0" layoutY="584.0" pickOnBounds="true" smooth="false"
			visible="false">
			<image>
				<Image url="@Picture1.png" />
			</image>
		</ImageView>
		<Button fx:id="startbtn" layoutX="14.0" layoutY="14.0"
			mnemonicParsing="false" onAction="#StartHandle"
			style="-fx-background-color: #00004d; -fx-border-color: white;"
			text="Start" textFill="WHITE">
			<font>
				<Font name="SignPainter-HouseScript" size="28.0" />
			</font>
		</Button>
		<Button fx:id="stopbtn" disable="true" layoutX="106.0"
			layoutY="14.0" mnemonicParsing="false" onAction="#StopHandle"
			style="-fx-background-color: #00004d; -fx-border-color: white;"
			text="Stop" textFill="WHITE">
			<font>
				<Font name="SignPainter-HouseScript" size="28.0" />
			</font>
		</Button>
		<ImageView fx:id="thinkingimg" fitHeight="46.0"
			fitWidth="50.0" layoutX="836.0" layoutY="594.0" pickOnBounds="true"
			smooth="false" visible="false">
			<image>
				<Image url="@thinking.png" />
			</image>
		</ImageView>
		<ImageView fx:id="solimg" fitHeight="46.0" fitWidth="50.0"
			layoutX="836.0" layoutY="594.0" pickOnBounds="true" smooth="false"
			visible="false">
			<image>
				<Image url="@sol.png" />
			</image>
		</ImageView>
	</children>
</AnchorPane>
